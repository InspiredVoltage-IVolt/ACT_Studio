<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>disable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <BaseOutputPath>D:\IVolt_Releases\ACT_Studio\ACT.Studio.GUI</BaseOutputPath>
    <PackageId>ACT.Studio.GUI</PackageId>
    <Version>2.0.1</Version>
    <Authors>Mark Alicz</Authors>
    <Company>IVolt</Company>
    <Product>ACT Studio</Product>
    <Description>ACT Studio - Manage Everything About ACT and Your Installation</Description>
    <Copyright>2022</Copyright>
    <PackageLicenseExpression>  AGPL-3.0-or-later</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Guna.Charts.WinForms" Version="1.0.3" />
    <PackageReference Include="Guna.UI2.WinForms" Version="2.0.3.2" />
  </ItemGroup>

</Project>